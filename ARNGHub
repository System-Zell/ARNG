local GameID = 17461365630
local Plr = game.Players.LocalPlayer
print("Script Executed "..game.PlaceId)


if game.PlaceId == GameID then
	print("Executing Admin RNG Hub")
	local DoubleSpinDB = false
	local AutoSpinDB = false
	
	local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/slf0Dev/Ocerium_Project/main/Library.lua"))()
	Window = Library.Main("Admin RNG Hub | Private","LeftAlt")
	
	local MainTab = Window.NewTab("üè° Home")
	local MainSection = MainTab.NewSection("Main - Auto Spin")
	
	local DoubleSpinToggle = MainSection.NewToggle("Double Spin",function(Value)
		print("The user toggled Double Spin to "..tostring(Value))

		if Value == true then
			DoubleSpinDB = true

		elseif Value == false then
			DoubleSpinDB = false
		end
	end,false) 
	
	local AutoSpinToggle = MainSection.NewToggle("Auto Spin",function(Value)
		print("The user toggled Double Spin to "..tostring(Value))

		if Value == true then
			AutoSpinDB = true
			while AutoSpinDB == true do
				wait(0.05)

				local args = {
					[1] = true,
					[2] = DoubleSpinDB
				}

				game:GetService("ReplicatedStorage").Events.Spin:InvokeServer(unpack(args))
			end
		elseif Value == false then
			AutoSpinDB = false
		end
	end,false) 
end
