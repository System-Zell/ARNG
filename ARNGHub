				end
			},
		},
	})
	
	local TwiceSpinToggle = MainTab:CreateToggle({
		Name = "2X Spin",
		CurrentValue = false,
		Flag = "ToggleTwiceSpin",
		Callback = function(Value)
			print("The user toggled TwiceSpin to "..tostring(Value))

			if Value == true then
				TwiceSpin = true

			elseif Value == false then
				TwiceSpin = false
			end
		end,
	})
	
	TwiceSpinToggle:Set(false)
	
	local AutoSpinToggle = MainTab:CreateToggle({
		Name = "AutoSpin",
		CurrentValue = false,
		Flag = "ToggleAutoSpin",
		Callback = function(Value)
			print("The user toggled AutoSpin to "..tostring(Value))
			
			if Value == true then
				while Value == true do
					wait(0.05)
					
					local args = {
						[1] = true,
						[2] = TwiceSpin
					}

					game:GetService("ReplicatedStorage").Events.Spin:InvokeServer(unpack(args))
				end
				
			elseif Value == false then
				
			end
		end,
	})
end
